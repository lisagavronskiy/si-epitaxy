dump dump_eq all custom 100 ${filename}_equil.lammpstrj id type element x y z
dump dump_eq all custom 100 si_64x64x64_5defectsperlayer_equil.lammpstrj id type element x y z
dump_modify dump_eq element Si

# Define Thermodynamic Outputs
